<jnlp spec="6.0+" codebase="http://www.cogsys.cs.uni-tuebingen.de/software/KEGGtranslator/downloads/" href="kgtrans.jnlp">
   <information>
    <title>KEGGtranslator</title>
    <vendor>University of Tuebingen</vendor>
    <description kind="short">KEGGtranslator</description>
    <description>Precise generation of systems biology models from KEGG pathways</description>
    <homepage href="http://www.cogsys.cs.uni-tuebingen.de/software/KEGGtranslator/downloads/"/>
    <icon href="http://www.cogsys.cs.uni-tuebingen.de/software/KEGGtranslator/images/KEGGtranslatorIcon_48.png"/> 
    <icon kind="shortcut" href="http://www.cogsys.cs.uni-tuebingen.de/software/KEGGtranslator/images/KEGGtranslatorIcon_48.png" width="16" height="16"/>
    <icon kind="splash" href="http://www.cogsys.cs.uni-tuebingen.de/software/KEGGtranslator/images/splash.gif"/> 
    <offline-allowed/>
  </information>
 <resources>
    <j2se version="1.6+" initial-heap-size="64m" max-heap-size ="256m" java-vm-args="-splash:bin/de/zbit/kegg/gui/img/Logo.gif" href="http://java.sun.com/products/autodl/j2se"/>
    <jar href="[JAR_NAME]" main="true"/>
 </resources>
  <security>
    <all-permissions/>
  </security>
 <application-desc main-class="de.zbit.kegg.Translator"/>
</jnlp>
