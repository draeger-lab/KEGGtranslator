  <LaunchCommandWin>cmd /c start javaw.exe -Xms6G -Xmx6G -jar "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommandWin>
  <LaunchCommandMac>java -Xms6G -Xmx6G -jar "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommandMac>
